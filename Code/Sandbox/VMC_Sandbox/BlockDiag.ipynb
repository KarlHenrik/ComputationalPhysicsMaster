   "source": [
    "n = 200\n",
    "\n",
    "C = zeros(2n, 2n)\n",
    "B1 = rand(n, n)\n",
    "B2 = rand(n, n)\n",
    "\n",
    "C[1:n, 1:n] .= B1\n",
    "C[n+1:end, n+1:end] = B2\n",
    "\n",
    "@time la.det(C)\n",
    "@time la.det(B1) * la.det(B2)"
   ]
